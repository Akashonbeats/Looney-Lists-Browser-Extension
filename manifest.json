{
  "manifest_version": 3,
  "name": "Looney Lists",
  "version": "3.1",
  "description": "An aesthetically pleasing simple to use To-Do List App",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon17.png",
      "48": "icons/icon42.png",
      "128": "icons/icon42.png"
    }
  },
  "icons": {
    "16": "icons/icon17.png",
    "48": "icons/icon42.png",
    "128": "icons/icon42.png"
  }
}
